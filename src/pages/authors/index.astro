---
import Contributors from '@components/Contributors.astro';
import siteMetadata from '@constants/siteMetadata';
import BaseLayout from '@layouts/BaseLayout.astro';
import { getAllAuthors } from '@utils/authors';

const authors = await getAllAuthors();
---

<BaseLayout title={`Authors - ${siteMetadata.author}`} description={siteMetadata.description}>
  <Contributors contributors={authors} />
</BaseLayout>
