---
import BaseLayout from '@layouts/BaseLayout.astro';
import JobGrid from '@components/JobGrid';
import MeetupsGrid from '@components/MeetupsGrid';
import { getLatestJobs } from '@utils/jobs';
import '../styles/global.css';
import { getMeetups } from '@utils/meetups';

const title = 'Home Page';
const { jobs } = await getLatestJobs(9);
const events = await getMeetups();
---

<BaseLayout title={title}>
  <MeetupsGrid events={events} />
  <JobGrid jobs={jobs} />
</BaseLayout>
